
const sampleInt8 = () => crypto.getRandomValues(new Int8Array(1))[0]
const sampleInt16 = () => crypto.getRandomValues(new Int16Array(1))[0]
const sampleInt32 = () => crypto.getRandomValues(new Int32Array(1))[0]
const sampleBoolean = () => !!mod(sampleInt8(), 2)
const sampleArray = (...array: any[]) => {
    const random = sampleInt32()
    const index = mod(random, array.length)
    return array[index]
}